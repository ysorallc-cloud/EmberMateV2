{
  "name": "embermate",
  "version": "2.0.0",
  "description": "Your Personal Health Companion for managing medications, vitals, and wellness - Now with modern landing page",
  "main": "landing.html",
  "scripts": {
    "start": "npx http-server -p 8000 -o landing.html",
